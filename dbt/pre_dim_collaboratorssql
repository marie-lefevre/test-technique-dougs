SELECT 
    col.id,
    col.user_id,
    col.team_id,
    col.first_name,
    col.last_name,
    CONCAT(col.first_name, ' ', col.last_name) AS full_name,
    REPLACE(SUBSTR(col.skills, 2, LENGTH(col.skills)-2), "'", "") AS skills,
    col.created_at,
    col.suspended_at,
    col.productivity,
    col.intercom_preference,
    col._etl_loaded_at
FROM {{ ref('stg_collaborators') }} AS col